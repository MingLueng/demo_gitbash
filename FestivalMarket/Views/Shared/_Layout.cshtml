<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="robots" content="max-snippet:-1, max-image-preview:large, max-video-preview:-1" />

    @RenderSection("metaTags", required: false)


    <title>Festival Market - Siêu Thị Lễ Hội đầu tiên tại Nha Trang</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw==" crossorigin="anonymous" referrerpolicy="no-referrer" />


    <link rel="stylesheet" href="~/Content/assets/styles/fonts.css">
    <link rel="stylesheet" href="~/Content/assets/styles/layout.css">
    <link rel="stylesheet" href="~/Content/assets/styles/member.css">
    <link rel="stylesheet" href="~/Content/assets/styles/responsive.css">
    <link rel="stylesheet" href="~/Content/assets/styles/khuyen-mai.css">
    <link rel="stylesheet" href="~/Content/assets/styles/festival-go.css">
    <link rel="stylesheet" href="~/Content/assets/styles/su-kien.css">
    <link rel="stylesheet" href="~/Content/assets/styles/news.css">
    <link rel="stylesheet" href="~/Content/assets/styles/home.css">

    <link rel="stylesheet" href="~/Content/slick/slick-theme.css">
    <link rel="stylesheet" href="~/Content/slick/slick.css">


    
        @*@Scripts.Render("~/bundles/modernizr")

          @Scripts.Render("~/bundles/jqueryval")*@
</head>
<body onscroll="myFunction()">
    <header class="">
        <div class="top-header bg-cam px-57 py-2" id="top-head">
            <div class="row _header-flex ">
                <div class="col-md-6  col-12 __header-flex ">
                    <div class="address-head">
                        <span><i class="fa-solid fa-location-dot color-wh"></i></span>
                        <span class="text-white ml-2">Tìm cửa hàng gần bạn</span>
                    </div>
                    <div class="time-head d-flex color-white">
                        <div class="time-start d-flex align-items-center mr-4">
                            <span><i class="fa-solid fa-clock color-wh "></i></span>
                            <span class="ml-2 text-white">Giờ mở cửa:</span>
                        </div>
                        <div class="">
                            <div>
                                <span class="text-white">Siêu thị:</span> <span class="text-white">6:00 - 23:00 (T2-CN)</span>
                            </div>
                            <div><span class="text-white">Cửa hàng:</span> <span class="text-white">0:00 - 24:00 (T2-CN)</span></div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-12  __header-flex-right">
                    <div class="lang-head-content">
                        <div class="active">
                            VN
                        </div>
                        <div>
                            EN
                        </div>
                    </div>
                    <div class="search-input">
                        <div class="d-flex form-search align-items-center">
                            <i class="fa-sharp font-13 text-white color-white fa-solid fa-magnifying-glass"></i>
                            <input type="search" class="p-2">
                        </div>
                    </div>
                    <div class="icon-head">
                        <div class="d-flex justify-content-between ">
                            <span class="active"><i class="fa-brands text-white fa-facebook "></i></span>
                            <span><i class="fa-brands text-white fa-instagram"></i></span>
                            <span><i class="fa-brands text-white fa-youtube"></i></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="bot-header px-57">
            <nav class="navbar navbar-expand-lg navbar-light">
                <a class="navbar-brand" href="/trang-chu">
                    <img src="~/Content/assets/images/festivalmarket/Logo.png" alt="logo">
                </a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav d-flex justify-content-end w-100">
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="/tin-tuc" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                TIN TỨC
                            </a>

                            @Html.Action("MenuTop", "Menu")
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/khuyen-mai">KHUYẾN MÃI</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/su-kien">SỰ KIỆN</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/thanh-vien">CHƯƠNG TRÌNH THÀNH VIÊN</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/festival-go">FESTIVAL GO</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/lien-he">LIÊN HỆ</a>
                        </li>
                    </ul>

                </div>
            </nav>
        </div>
    </header>

    @RenderBody();


    <!-- footer  -->
    <footer class="footer bg-cam px-57">
        <div class="row align-items-center">
            <div class="col-md-2 col-sm-4 col-12">
                <div class="logo-footer">
                    <div class="d-flex justify-content-center mx-auto logo-wh">
                        <img src="~/Content/assets/images/festivalmarket/Logo-wh.png" alt="logo">
                    </div>
                    <div>
                        <div class="icon-head">
                            <div class="d-flex justify-content-center ">
                                <span class="active"><i class="fa-brands text-white fa-facebook "></i></span>
                                <span><i class="fa-brands text-white fa-instagram"></i></span>
                                <span><i class="fa-brands text-white fa-youtube"></i></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4  col-sm-8 col-12">
                <div class="menu-footer row">
                    <div class="col-6">
                        <ul class="menu-bot">
                            <li class="item-footer">
                                <a href="/tin-tuc">Tin tức</a>
                            </li>
                            <li class="item-footer">
                                <a href="/khuyen-mai">Khuyến mại</a>
                            </li>
                            <li class="item-footer">
                                <a href="/thanh-vien">Chương trình thành viên</a>
                            </li>
                        </ul>
                    </div>
                    <div class="col-6">
                        <ul class="menu-bot">
                            <li class="item-footer">
                                <a href="/su-kien">Sự kiện</a>
                            </li>
                            <li class="item-footer">
                                <a href="/festival-go">Festival Go</a>
                            </li>
                            <li class="item-footer">
                                <a href="/lien-he">Liên hệ</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-md-6   col-12">
                <div class="sign-up">
                    <div class="top-sign">
                        <h3 class="font-25 text-white pt-3">
                            Newsletter Sign Up
                        </h3>
                        <input type="text" class="input-sign ">
                    </div>
                    <div class="add-footer">
                        <ul>
                            <li class="detail-add">
                                <span>Địa chỉ:</span> <span class="ml-2"> 3-5 Phạm Văn Đồng, thành phố Nha Trang</span>
                            </li>
                            <li class="detail-add">
                                <span>Hotline:</span> <span class="ml-2"> 089 808 9586</span>
                            </li>
                            <li class="detail-add">
                                <span>Email:</span> <span class="ml-2"> cskh@festivalmarket.vn</span>
                            </li>

                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </footer>

    <!-- script -->

    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.7/dist/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <script src="~/Content/slick/slick.js"></script>
    <script src="~/Content/assets/js/common.js"></script>



    @RenderSection("scripts", required: false)

</body>
</html>

